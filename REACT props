import React from 'react';

// Um componente funcional simples que recebe props
const MeuComponente = (props) => {
  return (
    <div>
      <h1>{props.titulo}</h1>
      <p>{props.texto}</p>
    </div>
  );
};

// Um componente pai que renderiza o componente filho com props
const App = () => {
  return (
    <div>
      {/* Passando props para o componente MeuComponente */}
      <MeuComponente titulo="Título do Componente" texto="Este é um exemplo de utilização de props." />
    </div>
  );
};

export default App;
